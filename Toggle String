#include <stdio.h>
#include <string.h>
void main()
{
    char A[102];
    int l, i;
    gets(A);
    l = strlen(A);
    for(i=0; i<l; i++)
    {
        if(A[i]>=97 && A[i]<=122)
        {
            A[i] = A[i] - 32;
            continue;
        }
        if(A[i]<=90 && A[i]>=65)
        {
            A[i] = A[i] + 32;
        }
    }
    printf("%s", A);
    
    
}
